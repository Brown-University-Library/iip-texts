               <!-- ********* <physDesc> ********* -->
               <physDesc><!-- ********* <objectDesc> ********* -->
                  <objectDesc ana="#column">
                     <supportDesc ana="marble">
                        <support>
                           <dimensions type="surface" unit="cm">
                              <height>255</height>
                              <width>56</width>
                           </dimensions>
                        </support>
                        <condition ana="fragment.single">
                           <p>Upper section of a column. The inscribed surface is badly worn: this
                              part of the column seems to have been submerged in water for a long
                              time. The height includes the integral necking</p>
                        </condition>
                     </supportDesc>
                     <!-- ********* <layoutDesc> ********* -->
                     <layoutDesc>
                        <layout columns="1" writtenLines="5">
                           <p>The inscription is located at the bottom of the surviving section</p>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <!-- ********* <handDesc> ********* -->
                  <handDesc>
                     <handNote ana="#engraved">
                        <dimensions type="letter" extent="height" unit="cm" atLeast="4.5" atMost="6"/>
                        <p>The letters are somewhat irregular and cursive. Alpha has a slanted crossbar, upsilon is stemmed,
                           mu is exaggerately cursive</p>
                     </handNote>
                  </handDesc>
                  <!-- ********* <decoDesc> ********* -->
                  <decoDesc>
