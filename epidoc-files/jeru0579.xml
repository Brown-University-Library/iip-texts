                    <expan><abbr>leg</abbr><ex>ionis</ex></expan> <num value="2">II</num> <expan><abbr>Aug</abbr><ex>ustae</ex></expan>, <expan><abbr>leg</abbr><ex>ionis</ex></expan> <num value="20">XX</num> <lb/> <expan><abbr>Vic</abbr><ex>tricis</ex></expan>,  <expan><abbr>leg</abbr><ex>ionis</ex></expan> <num value="2">II</num> <expan><abbr>Aug</abbr><ex>ustae</ex></expan>,
