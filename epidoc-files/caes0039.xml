               <!-- ********* <msContents> ********* -->
               <msContents>
                  <textLang mainLang="grc"/>
                  <msItem ana="#other_religion" class="#acclamation">
                     <p>Caesarea, 560 CE - 610 CE. Pavement Mosaic. Acclamation.</p>
                  </msItem>
               </msContents>
               <!-- ********* <physDesc> ********* -->
               <physDesc><!-- ********* <objectDesc> ********* -->
                  <objectDesc ana="pavement #mosaic">
                     <supportDesc ana="stone.unknown">
                        <support>
                           <dimensions type="surface" unit="cm">
                              <height>110</height>
                              <width>103</width>
                           </dimensions>
                           <p>Diamensions refer to the diameter</p>
                        </support>
                        <condition ana="complete.broken">
                           <p>Damaged at the top</p>
                        </condition>
                     </supportDesc>
                     <!-- ********* <layoutDesc> ********* -->
                     <layoutDesc>
                        <layout columns="1" writtenLines="5">
                           <p>Single rows of black tesserae in a white field delineate the border
                              and the letters</p>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <!-- ********* <handDesc> ********* -->
                  <handDesc>
                     <handNote ana="inset.mosaic">
                        <dimensions type="letter"
                                    extent="height"
                                    unit="cm"
                                    atLeast="10"
                                    atMost="17"/>
                        <p>The letters are somewhat irregular</p>
                     </handNote>
                  </handDesc>
                  <!-- ********* <decoDesc> ********* -->
                  <decoDesc>
                     <decoNote ana="#xx">
                        <ab>Medallion</ab>
                        <locus>Contains the inscription</locus>
                     </decoNote>
                  </decoDesc>
               </physDesc>
               <!-- ********* <history> ********* -->
               <history>
                  <origin>
                     <date period="http://n2t.net/ark:/99152/p0m63njjcn4"
                           notBefore="0560"
                           notAfter="0610">560 CE - 610 CE</date>
                     <placeName>
                        <region>Coastal Plain</region>
                        <settlement ref="http://pleiades.stoa.org/places/678401">Caesarea</settlement>
                        <geogName type="site"/>
                     </placeName>
                     <p>Found in an oval room south of Old City and west of the Imperial Revenue
                        Office. The inscription is oriented with the bottom toward the northwest,
                        facing one of the entrances to the room.</p>
                  </origin>
                  <provenance>
                     <placeName>In situ</placeName>
                     <date/>
                  </provenance>
               </history>
            </msDesc>
