                        <msItem class="dedicatory.imperial" ana="christian">
                            <p>Jerusalem, Church of the Holy Sepulchre, 586-602 CE. Column Capitals.
                                Imperial Dedication.</p>
                        </msItem>
                    </msContents>
                    <!--  *************************************  <physDesc>  ********************************* -->
                    <physDesc>
                        <objectDesc ana="capital">
                            <supportDesc ana="marble">
