                                    <p>Inscription is within a medallion on the capital.</p>
                                    <dimensions type="surface" unit="cm">
                                        <height>27.5</height>
                                        <width>23</width>
                                        <depth/>
                                    </dimensions>
                                </support>
                                <condition ana="complete.broken">
                                    <p>There are two identical capital medallions, but they are
                                        defaced by some fire.</p>
                                </condition>
                            </supportDesc>
                            <layoutDesc>
                                <layout columns="1" writtenLines="4">
                                    <p/>
                                </layout>
                            </layoutDesc>
                        </objectDesc>
