                    <physDesc>
                        <objectDesc ana="#tombstone">
                            <supportDesc ana="#stone.other">
                                <support>
                                    <p>White and purple sandstone, almost square in shape</p>
                                    <dimensions type="surface" unit="cm">
                                        <height/>
                                        <width/>
                                        <depth/>
                                    </dimensions>
                                </support>
                                <condition ana="#complete.broken">
                                    <p>Broken at the lower right corner. The inscribed surface is
                                        flaked off at the top and at the bottom</p>
                                </condition>
                            </supportDesc>
