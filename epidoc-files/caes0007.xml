               <!-- ********* <physDesc> ********* -->
               <physDesc><!-- ********* <objectDesc> ********* -->
                  <objectDesc ana="#column">
                     <supportDesc ana="marble">
                        <support>
                           <dimensions type="surface" unit="cm">
                              <height>142</height>
                              <width>39-43</width>
                           </dimensions>
                           <p>The diameter is 39 cm at the top, 43 cm at the bottom</p>
                        </support>
                        <condition ana="complete.broken">
                           <p>Some damage at the lower back, which however does not interfere with
                              the inscription</p>
                        </condition>
                     </supportDesc>
                     <!-- ********* <layoutDesc> ********* -->
                     <layoutDesc>
                        <layout columns="1" writtenLines="5">
                           <p/>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <!-- ********* <handDesc> ********* -->
                  <handDesc>
                     <handNote ana="#engraved">
                        <dimensions type="letter"
                                    extent="height"
                                    unit="cm"
                                    atLeast="4.5"
                                    atMost="8.5"/>
                        <p>The first three lines are in the monumental alphabet used throughout the Roman period at
                           Caesarea, while lines 4-5 are in the smaller, more cursive script of the
                           second and third centuries. The letters are large and deeply cut. Dots or
                           short dashes divide some words. H and E near the end of the second line
                           are ligated and the 7-shaped symbols denotes the word: "centurio".</p>
                     </handNote>
                  </handDesc>
