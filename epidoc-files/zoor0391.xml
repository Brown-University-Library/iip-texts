               <!-- ********* <msContents> ********* -->
               <msContents>
                  <textLang mainLang="grc"/>
                  <msItem class="#funerary.epitaph" ana="#christian">
                     <p>Zoora, mid-5th century CE. Tombstone. Funerary (Epitaph).</p>
                  </msItem>
               </msContents>
               <!-- ********* <physDesc> ********* -->
               <physDesc>
                  <!-- ********* <objectDesc> ********* -->
                  <objectDesc ana="#tombstone">
                     <supportDesc ana="#stone.other">
                        <support>
                           <p>Reddish sandstone, originally it should have been rectangular in shape
                              with a rounded top</p>
                           <dimensions type="surface" unit="cm">
                              <height>49</height>
                              <width>31</width>
                              <depth>7</depth>
                           </dimensions>
                        </support>
                        <condition ana="#complete.broken">
                           <p>The upper left part is missing. The inscribed surface is badly
                              weathered at its upper half, with obliteration of two-thirds of the
                              epitaph</p>
                        </condition>
                     </supportDesc>
                     <!-- ********* <layoutDesc> ********* -->
                     <layoutDesc>
                        <layout columns="1" writtenLines="5">
                           <p/>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <!-- ********* <handDesc> ********* -->
                  <handDesc>
                     <handNote ana="#impressed.inscribed">
                        <dimensions type="letter" extent="height" atLeast="1.2" atMost="2"/>
                        <p>Engraved in a mixture of round and square script. The letters are of
                           mediocre quality and alignment</p>
                     </handNote>
                  </handDesc>
                  <!-- ********* <decoDesc> ********* -->
                  <decoDesc>
                     <decoNote ana="#xx">
                        <ab>Traces of a deeply engraved cross</ab>
                        <locus>Above the text</locus>
                     </decoNote>
                     <decoNote>
                        <ab>Incised rectangular frame</ab>
                        <locus>Around the text. Only its lower part is fully preserved.</locus>
                     </decoNote>
                     <decoNote>
                        <ab>Linear cross with triangular serifs</ab>
                        <locus>At the beginning of the last line</locus>
                     </decoNote>
                     <decoNote>
                        <ab>Cross consisting of four free-standing triangular bars</ab>
                        <locus>At the end of the last line</locus>
                     </decoNote>
                  </decoDesc>
               </physDesc>
               <!-- ********* <history> ********* -->
               <history>
                  <origin>
                     <date period="http://n2t.net/ark:/99152/p0m63njjcn4"
                           notBefore="0430"
                           notAfter="0470">Mid-5th century CE</date>
                     <placeName>
                        <region>Negev</region>
                        <settlement ref="http://pleiades.stoa.org/places/697768">Zoora</settlement>
                        <geogName type="site">An Naq Cemetery</geogName>
                        <geogFeat type="locus"/>
                     </placeName>
                     <p>Found in the northwest corner of the Bronze Age, Byzantine and Islamic
                        cemetery in the An Naq neighborhood south of the Wadi al-Hasa, probably in
                        secondary use in later graves.</p>
                  </origin>
                  <provenance>
                     <placeName/>
                     <date/>
                  </provenance>
               </history>
