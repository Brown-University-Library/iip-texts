            <div type="edition" subtype="transcription" ana="b1">
                <p><expan><abbr>D</abbr><ex>is</ex></expan>
                    <expan><abbr>M</abbr><ex>anibus</ex></expan>. <lb/>
                    <expan><abbr>M</abbr><ex>arco</ex></expan> Ulpio <expan><abbr>Cl</abbr><ex>audia
                            tribu</ex></expan>, <lb/> Magno
                        <expan><abbr>S<hi rend="ligature">av</hi></abbr><ex>ariensi</ex></expan>, <lb/> Centurioni
                            <expan><abbr>Leg</abbr><ex>ionis</ex></expan>
                    <num value="5"><hi rend="supraline">V</hi></num>
                    <expan><abbr>Mac</abbr><ex>edonicae</ex></expan>
                    <lb/>
                    <expan><abbr>H</abbr><ex>ic</ex></expan>
                    <expan><abbr>s</abbr><ex>itus</ex></expan>
                    <expan><abbr>e</abbr><ex>st</ex></expan>
                    <lb/>
