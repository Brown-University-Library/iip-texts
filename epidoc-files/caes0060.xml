                  <msItem ana="unknown_religion" class="#honorific">
                     <p>Caesarea, 200 CE - 650 CE. Archivolt blocks. Honorific.</p>
                  </msItem>
               </msContents>
               <!-- ********* <physDesc> ********* -->
               <physDesc><!-- ********* <objectDesc> ********* -->
                  <objectDesc ana="#block">
                     <supportDesc ana="#marble">
                        <support>
                           <dimensions type="surface"/>
                           <p>Marble archivolt blocks.</p>
                        </support>
                        <condition ana="fragments">
                           <p>Three archivolt blocks, all pointed on the top, bottom and back, and
                              more smoothly finished in front and on ends. They belong to the same
                              arch, which would have spanned about 2 m. Block b joins the right side
                              of a, but c fits elsewhere. The inscription presumably belongs to a
                              second use, when the blocks were installed in a different sequence</p>
                        </condition>
                     </supportDesc>
                     <!-- ********* <layoutDesc> ********* -->
                     <layoutDesc>
                        <layout columns="1" writtenLines="1">
                           <p>The inscription is cut into the top fascia</p>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
