                        <msItem class="honorific" ana="christian">
                            <p>Caesarea, 6th century CE. Plaster. Honorific.</p>
                        </msItem>
                    </msContents>
                    <!--  *************************************  <physDesc>  ********************************* -->
                    <physDesc>
                        <objectDesc ana="other_object">
                            <supportDesc ana="plaster">
                                <support>
                                    <p>Likely part of a cross. </p>
                                    <dimensions type="surface" unit="cm">
                                        <height/>
                                        <width/>
                                        <depth/>
                                    </dimensions>
                                </support>
                                <condition ana="fragments.non_contig">
                                    <p>The two fragments almost certainly belonged to the same
                                        cross.</p>
                                </condition>
                            </supportDesc>
                            <layoutDesc>
                                <layout columns="2" writtenLines="2">
                                    <p>Fragment 2a contains two letters in the obverse, and Fragment
                                        2b contains one letter. </p>
                                </layout>
                            </layoutDesc>
                        </objectDesc>
                        <!--  *************************************  <handDesc>  ********************************* -->
                        <handDesc>
                            <handNote ana="painted.brush">
                                <dimensions type="letter" extent="height" unit="cm" atLeast="12.5"
                                    atMost="13.4"/>
                                <p>Letters were painted in red, and both letters were written with
                                    seriphs and curls for decoration.</p>
                            </handNote>
                        </handDesc>
                        <!--  *************************************  <decoDesc>  ********************************* -->
                        <decoDesc>
                            <decoNote>
                                <ab>Remains of the left arm of a crux gemmata. </ab>
                                <locus>Beneath inscription on Fragment 2a, obverse.</locus>
                            </decoNote>
                            <decoNote>
                                <ab>Remains of the left arm and central stem of a crux gemmata. </ab>
                                <locus>Above inscription on Fragment 2b, obverse.</locus>
                            </decoNote>
                        </decoDesc>
