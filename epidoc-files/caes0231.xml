               <!-- ********* <physDesc> ********* -->
               <physDesc><!-- ********* <objectDesc> ********* -->
                  <objectDesc ana="#plaque">
                     <supportDesc ana="#marble">
                        <support>
                           <dimensions type="surface" unit="cm">
                              <height>23</height>
                              <width>16</width>
                              <depth>1.4</depth>
                           </dimensions>
                        </support>
                        <condition ana="fragment.single">
                           <p>Large piece of the right side of a gray marble plaque</p>
                        </condition>
                     </supportDesc>
                     <!-- ********* <layoutDesc> ********* -->
                     <layoutDesc>
                        <layout columns="1" writtenLines="6">
                           <p>The ends of all six lines survive, and a dove flanked by two crosses
                              is carved at the end of line 6</p>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <!-- ********* <handDesc> ********* -->
                  <handDesc>
                     <handNote ana="#engraved">
                        <dimensions type="letter"
                                    extent="height"
                                    unit="cm"
                                    atLeast="1.8"
                                    atMost="3.6"/><p>Letters are highly irregular but deeply cut.</p>
                     </handNote>
                  </handDesc>
                  <!-- ********* <decoDesc> ********* -->
                  <decoDesc>
                     <decoNote ana="#xx">
                        <ab>Crosses </ab>
                        <locus>Bottom of the plaque, one to the left and one to the right of the
                           dove </locus>
                     </decoNote>
                     <decoNote ana="#xx">
                        <ab>Dove </ab>
                        <locus>Bottom of the plaque, between two crosses </locus>
                     </decoNote>
                  </decoDesc>
