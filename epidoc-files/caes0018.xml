               <lb/>Titi<supplied reason="lost">ano</supplied>
               <lb/>legato <expan><abbr>Au<supplied reason="lost">g</supplied></abbr><ex>usti</ex></expan>
               <supplied reason="lost"><expan><abbr>pr</abbr><ex>o</ex></expan></supplied>
