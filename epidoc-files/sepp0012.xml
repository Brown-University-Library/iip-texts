                  <msItem class="#dedicatory" ana="#jewish">
                     <p>Galilee. Sepphoris. 400-500 CE. Synagogue. Black and white floor mosaic. Dedication.</p>
                  </msItem>
               </msContents>
               <!-- ********* <physDesc> ********* -->
               <physDesc><!-- ********* <objectDesc> ********* -->
                  <objectDesc ana="#mosaic">
                     <supportDesc>
