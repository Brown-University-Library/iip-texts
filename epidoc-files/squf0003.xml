            <p>
               <gap reason="illegible" unit="character" quantity="1"/>ΠΙΤΟΥΘΕΟΣ<gap reason="lost" unit="character" extent="unknown"/>
               <lb/>ΙΛΙΑΑΡΧ <gap reason="illegible" quantity="1" unit="character"/>ΜΑ<gap reason="lost" unit="character" extent="unknown"/>
               <lb/>SΙΑΝΗΣΜΑΘ<gap reason="lost" unit="character" extent="unknown"/>
