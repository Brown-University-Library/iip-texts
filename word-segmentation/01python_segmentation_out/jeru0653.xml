                        <msItem class="label text_other" ana="unknown_religion">
                            <p>Jerusalem, 4th to 6th century CE. Clay stamps. Signatures of
                                workers.</p>
                        </msItem>
                    </msContents>
                    <!--  *************************************  <physDesc>  ********************************* -->
                    <physDesc>
                        <objectDesc ana="stamp">
                            <supportDesc ana="clay">
                                <support>
                                    <p>Petrographic analysis indicates the stamps consist of red
                                        clay mixed with dolomite particles. The origin of the two
                                        materials, Moza formation and Aminadav formation
                                        respectively, indicate that they were all made around
                                        Jerusalem. </p>
                                    <dimensions type="surface" unit="cm">
                                        <height>4-5</height>
                                        <width>6.3-9.5</width>
                                        <depth/>
                                    </dimensions>
                                </support>
                                <condition ana="fragments">
                                    <p/>
                                </condition>
                            </supportDesc>
                            <layoutDesc>
                                <layout columns="1" writtenLines="8">
                                    <p>8 different brick tiles found together, with different
                                        names.</p>
                                </layout>
                            </layoutDesc>
                        </objectDesc>
