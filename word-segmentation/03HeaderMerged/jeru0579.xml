                    <expan><abbr>V</abbr><ex>ictricis</ex></expan>,  <expan><abbr>leg</abbr><ex>ionis</ex></expan> <num value="12">XII</num> <expan><abbr>Ful</abbr><ex>minatae</ex></expan>, <lb/>  <expan><abbr>leg</abbr><ex>ionis</ex></expan> <num value="10">X</num> <expan><abbr>Fr</abbr><ex>etensis</ex></expan>, <num value="3">III</num>
