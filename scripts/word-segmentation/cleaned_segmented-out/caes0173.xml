               <!-- ********* <msContents> ********* -->
               <msContents>
                  <textLang mainLang="grc"/>
                  <msItem class="#funerary" ana="#jewish">
                     <p>Caesarea, Date unknown. Plaque. Funerary.</p>
                  </msItem>
               </msContents>
               <!-- ********* <physDesc> ********* -->
               <physDesc><!-- ********* <objectDesc> ********* -->
                  <objectDesc ana="#plaque">
                     <supportDesc ana="#marble">
                        <support>
                           <dimensions type="surface" unit="cm">
                              <height>26</height>
                              <width>27</width>
                              <depth>3.7</depth>
                           </dimensions>
                        </support>
                        <condition ana="#complete">
                           <p>The plaque is somewhat waterworn, and marine deposits encrust parts of
                              the stone. The back has a coat of white plaster 6 mm thick, with
                              inclusions of charcoal and bits of pottery. The front is irregular but
                              smooth, the back and sides rough.</p>
                        </condition>
                     </supportDesc>
                     <!-- ********* <layoutDesc> ********* -->
                     <layoutDesc>
                        <layout columns="1" writtenLines="3">
                           <p/>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <!-- ********* <handDesc> ********* -->
                  <handDesc>
                     <handNote ana="#engraved">
                        <dimensions type="letter" extent="height" unit="cm" atLeast="2.8" atMost="4"/>
                        <p>Letters are shallow, irregular, and poorly cut.</p>
                     </handNote>
                  </handDesc>
                  <!-- ********* <decoDesc> ********* -->
                  <decoDesc>
                     <decoNote ana="#xx">
                        <ab> Menorah </ab>
                        <locus> End of inscription at the bottom of the plaque </locus>
                     </decoNote>
                     <decoNote ana="#xx">
                        <ab> Etrog, Shofar, Lulav? </ab>
                        <locus> End of inscription at the bottom of the plaque </locus>
                     </decoNote>
                  </decoDesc>
               </physDesc>
               <!-- ********* <history> ********* -->
               <history>
                  <origin>
                     <date period="Unknown">Date Unknown</date>
                     <placeName>
                        <region>Coastal Plain</region>
                        <settlement ref="http://pleiades.stoa.org/places/678401">Caesarea</settlement>
                        <geogName type="site"/>
                     </placeName>
                     <p>East of the city, in an area Lifshitz identified as the Jewish necropolis.</p>
                  </origin>
                  <provenance>
                     <placeName> Caesarea Museum </placeName>
                     <date/>
                  </provenance>
               </history>
