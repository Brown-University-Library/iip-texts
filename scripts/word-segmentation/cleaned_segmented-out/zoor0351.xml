                  <textLang mainLang="grc"/>
                  <msItem class="#funerary.epitaph" ana="#christian">
                     <p>Zoora, 375-400 CE. Tombstone. Funerary (Epitaph).</p>
                  </msItem>
               </msContents>
               <!-- ********* <physDesc> ********* -->
               <physDesc><!-- ********* <objectDesc> ********* -->
                  <objectDesc ana="#tombstone">
                     <supportDesc ana="#material_other">
                        <support>
                           <p>White-brownish sandstone, with three horizontal white stripes, nearly
                              square in shape</p>
                           <dimensions type="surface" unit="cm">
                              <height>27</height>
                              <width>38</width>
                              <depth/>
                           </dimensions>
                        </support>
                        <condition ana="#complete">
                           <p>The inscribed surface has been roughly smoothed</p>
                        </condition>
                     </supportDesc>
                     <!-- ********* <layoutDesc> ********* -->
                     <layoutDesc>
                        <layout columns="1" writtenLines="4">
                           <p/>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <!-- ********* <handDesc> ********* -->
                  <handDesc>
                     <handNote ana="#impressed.inscribed">
                        <dimensions type="letter" extent="height" atLeast="1.6" atMost="3.5"/>
                        <p>Square script with a rhomboid omicron and theta, and upsilon with an
                           ornamental horizontal stroke in the middle. The letters are carefully
                           carved, but present a descending alignment</p>
                     </handNote>
                  </handDesc>
                  <!-- ********* <decoDesc> ********* -->
                  <decoDesc>
                     <decoNote ana="#xx">
                        <ab>Cross-rho, slanting to right</ab>
                        <locus>In the lower right corner of the stone</locus>
                     </decoNote>
                     <decoNote ana="#xx">
                        <ab>Schematic palm branch</ab>
                        <locus>Next to the cross-rho monogram</locus>
                     </decoNote>
                  </decoDesc>
               </physDesc>
               <!-- ********* <history> ********* -->
               <history>
                  <origin>
                     <date period="http://n2t.net/ark:/99152/p0m63njjcn4" notBefore="0375" notAfter="0400">375 - 400 CE</date>
                     <placeName>
                        <region>Negev</region>
                        <settlement ref="http://pleiades.stoa.org/places/697768">Zoora</settlement>
                        <geogName type="site">An-Naq Cemetery</geogName>
                        <geogFeat type="locus"/>
                     </placeName>
                     <p>Found by local inhabitants in the northwest corner of the Bronze Age,
                        Byzantine and Islamic cemetery in the An Naq neighborhood south of the Wadi
                        al-Hasa, probably in secondary use in later graves.</p>
                  </origin>
                  <provenance>
                     <placeName>Department of Antiquities of Jordan</placeName>
                     <date/>
