                           <dimensions type="surface" unit="cm">
                              <height>39</height>
                              <width>24</width>
                              <depth>6</depth>
                           </dimensions>
                        </support>
                        <condition ana="#complete.broken">
                           <p>Rectangular in form, broken at the top and the lower left edge and
