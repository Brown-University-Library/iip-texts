               <!-- ********* <msContents> ********* -->
               <msContents>
                  <textLang mainLang="grc"/>
                  <msItem class="#funerary" ana="#christian">
                     <p>Caesarea, Fourth to mid seventh century CE. Plaque. Funerary.</p>
                  </msItem>
               </msContents>
               <!-- ********* <physDesc> ********* -->
               <physDesc><!-- ********* <objectDesc> ********* -->
                  <objectDesc ana="#plaque">
                     <supportDesc ana="#marble">
                        <support>
                           <dimensions type="surface" unit="cm">
                              <height>8</height>
                              <width>25</width>
                              <depth atLeast="2.2" atMost="2.3">2.2-2.3</depth>
                           </dimensions>
                        </support>
                        <condition ana="fragments">
                           <p>Top of a plaque of gray marble, mended from two fragments. Both faces
                              are polished and the sides are roughly cut</p>
                        </condition>
                     </supportDesc>
                     <!-- ********* <layoutDesc> ********* -->
                     <layoutDesc>
                        <layout columns="1" writtenLines="3">
                           <p>The first two lines and vestiges of the very end of a third survive.
                              Letters are crudely cut with a point and shallow. Alphas have broken
                              crossbars</p>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <!-- ********* <handDesc> ********* -->
                  <handDesc>
                     <handNote ana="#engraved">
                        <dimensions type="letter" extent="height" unit="cm" atLeast="1.4" atMost="3"/>
                     </handNote>
                  </handDesc>
                  <!-- ********* <decoDesc> ********* -->
                  <decoDesc>
                     <decoNote ana="#xx">
                        <ab>Cross </ab>
                        <locus>Beginning of line 1 </locus>
                     </decoNote>
                  </decoDesc>
               </physDesc>
               <!-- ********* <history> ********* -->
               <history>
                  <origin>
                     <date period="http://n2t.net/ark:/99152/p0m63njjcn4" notBefore="0300" notAfter="0650">Fourth to mid seventh century CE</date>
                     <placeName>
                        <region>Coastal Plain</region>
                        <settlement ref="http://pleiades.stoa.org/places/678401">Caesarea</settlement>
                        <geogName type="site"/>
                     </placeName>
                     <p>Found in the debris of a house 1.2 km northwest of the harbor.</p>
                  </origin>
                  <provenance>
                     <placeName> Israel Antiquities Authority, reg. no. 74-1847 </placeName>
                     <date/>
                  </provenance>
               </history>
