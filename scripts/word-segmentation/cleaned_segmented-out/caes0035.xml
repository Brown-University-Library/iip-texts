               <msContents>
                  <textLang mainLang="la"/>
                  <msItem ana="#other_religion" class="text_other">
                     <p>Caesarea, Second or third century CE. Slab. Other text (Senatorial
                        cursus).</p>
                  </msItem>
               </msContents>
               <!-- ********* <physDesc> ********* -->
               <physDesc><!-- ********* <objectDesc> ********* -->
                  <objectDesc ana="#slab">
                     <supportDesc ana="marble">
                        <support>
                           <dimensions type="surface" unit="cm">
                              <height>13</height>
                              <width>18</width>
                              <depth atLeast="1.8" atMost="2.0">1.8-2</depth>
                           </dimensions>
                        </support>
                        <condition ana="fragment.single">
                           <p>Broken on all sides. The back has traces of plaster, perhaps from
                              secondary use. </p>
                        </condition>
                     </supportDesc>
                     <!-- ********* <layoutDesc> ********* -->
                     <layoutDesc>
                        <layout columns="1" writtenLines="2">
                           <p>Small wedges are used as word dividers, and traces of red paint remain
                              in the letters</p>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <!-- ********* <handDesc> ********* -->
                  <handDesc>
                     <handNote ana="#engraved">
                        <dimensions type="letter" extent="height" unit="cm" atLeast="3.5" atMost="4.3"/>
                        <p>Letters are slightly cursive, with serifs. The alphabet indicates the second or the third century</p>
                     </handNote>
                  </handDesc>
                  <!-- ********* <decoDesc> ********* -->
                  <decoDesc>
                     <decoNote ana="#xx">
                        <ab/>
                        <locus/>
                     </decoNote>
                  </decoDesc>
               </physDesc>
               <!-- ********* <history> ********* -->
               <history>
                  <origin>
                     <date period="http://n2t.net/ark:/99152/p0m63njtk9m" notBefore="0100" notAfter="0300">Second or third century CE</date>
