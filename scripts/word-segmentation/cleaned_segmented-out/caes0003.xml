                  </msItem>
               </msContents>
               <!-- ********* <physDesc> ********* -->
               <physDesc><!-- ********* <objectDesc> ********* -->
                  <objectDesc ana="#statue_base">
                     <supportDesc ana="marble">
                        <support>
                           <dimensions type="surface" unit="cm">
                              <height>83</height>
                              <width>66</width>
                           </dimensions>
                        </support>
                        <condition ana="complete.broken">
                           <p>The base is damaged on its edges, and the surface is scratched and
                              gouged. The stone was further damaged after it was found and the first
                              four letters of line 1 are now missing. </p>
                        </condition>
                     </supportDesc>
                     <!-- ********* <layoutDesc> ********* -->
