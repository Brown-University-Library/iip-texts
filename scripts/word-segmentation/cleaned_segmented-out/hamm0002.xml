                  <msItem class="#dedicatory" ana="#christian">
                     <p>Galilee. Hammatha, September-November 455 CE. Slab. Dedicatory.</p>
                  </msItem>
               </msContents>
               <!-- ********* <physDesc> ********* -->
               <physDesc>
                  <!-- ********* <objectDesc> ********* -->
                  <objectDesc ana="#slab">
                     <supportDesc ana="#marble">
                        <support>
                           <dimensions type="surface" unit="cm">
                              <height>102</height>
                              <width>78</width>
                              <depth>3.8</depth>
                           </dimensions>
                           <p>Slab of white marble.</p>
                        </support>
                        <condition ana="fragments">
                           <p/>
                        </condition>
                     </supportDesc>
                     <!-- ********* <layoutDesc> ********* -->
                     <layoutDesc>
                        <layout columns="1" writtenLines="14">
                           <p>The longest line is 60 cm long</p>
                        </layout>
                     </layoutDesc>
