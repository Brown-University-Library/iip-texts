                  <msItem ana="#other_religion" class="#honorific">
                     <p>Caesarea, Late first to mid second century CE. Statue base. Honorific.</p>
                  </msItem>
               </msContents>
               <!-- ********* <physDesc> ********* -->
               <physDesc><!-- ********* <objectDesc> ********* -->
                  <objectDesc ana="#statue_base">
                     <supportDesc ana="kurkar">
                        <support>
                           <dimensions type="surface" unit="cm">
                              <height>24</height>
                              <width>68</width>
                           </dimensions>
                           <p>Dimensions refer to the inscribed area. The base itself measures 77 cm
                              in diameter and is 62 cm high</p>
                        </support>
                        <condition ana="complete.broken">
                           <p>The upper and lower moldings are slightly damaged and the surface of
                              the soft stone is badly pitted.</p>
                        </condition>
                     </supportDesc>
                     <!-- ********* <layoutDesc> ********* -->
                     <layoutDesc>
                        <layout columns="1" writtenLines="3">
                           <p/>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <!-- ********* <handDesc> ********* -->
                  <handDesc>
                     <handNote ana="#engraved">
                        <dimensions type="letter" extent="height" unit="cm" atLeast="55" atMost="60"/>
                        <p>Shallow cut wedges, some of which are now worn away, divide words. Letters are elongated, with
                           serifs, and are deeply and clearly cut, though now somewhat worn.</p>
                     </handNote>
                  </handDesc>
                  <!-- ********* <decoDesc> ********* -->
                  <decoDesc>
                     <decoNote ana="#xx">
                        <ab/>
                        <locus/>
                     </decoNote>
                  </decoDesc>
               </physDesc>
