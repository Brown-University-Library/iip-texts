         <div type="edition" ana="#b1" subtype="transcription" xml:id="caes0002.transcription" corresp="#caes0002.translation">
            <p><expan><abbr>T</abbr><ex>ito</ex></expan> Flavio <expan><abbr>Aug</abbr><ex>usti</ex></expan>
               <expan><abbr>liber</abbr><ex>to</ex></expan>
               <lb/>
               <expan><abbr>h</abbr><ex>onoris</ex></expan>
               <space extent="unknown" unit="character"/> Callisto <space extent="unknown" unit="character"/>
               <expan><abbr>c</abbr><ex>ausa</ex></expan>,<lb/>
               <expan><abbr>C</abbr><ex>aius</ex></expan> Aurunculeius amico suo.</p>
         </div>
         <div type="translation" ana="#b1" xml:id="caes0002.translation" corresp="#caes0002.transcription">
            <p>
               <name>
                  <persName xml:lang="la " ref="persons.xml#caes0002p01" nymRef="names.xml#caes0002n01">Gaius</persName>
                  <persName xml:lang="la " ref="persons.xml#caes0002p02" nymRef="names.xml#caes0002n02">Aurunculeius</persName>
               </name>
			 to his friend 
			 <name>
                  <persName xml:lang="la " ref="persons.xml#caes0002p03" nymRef="names.xml#caes0002n03">Titus</persName>
                  <persName xml:lang="la " ref="persons.xml#caes0002p04" nymRef="names.xml#caes0002n04">Flavius</persName>
                  <persName xml:lang="la " ref="persons.xml#caes0002p05" nymRef="names.xml#caes0002n05">Callistus</persName>
               </name>,
			 <rs type="status">freedman </rs> of <persName xml:lang="la " ref="persons.xml#caes0002p06">Augustus</persName>, out of respect.</p>
         </div>
      <div type="edition" subtype="transcription_segmented"><p><w xml:id="caes0002-1" xml:lang="la"><expan><abbr>T</abbr><ex>ito</ex></expan></w> <w xml:id="caes0002-2" xml:lang="la">Flavio</w> <w xml:id="caes0002-3" xml:lang="la"><expan><abbr>Aug</abbr><ex>usti</ex></expan></w> <w xml:id="caes0002-4" xml:lang="la"><expan><abbr>liber</abbr><ex>to</ex></expan></w> <w xml:id="caes0002-5" xml:lang="la"><expan><abbr>h</abbr><ex>onoris</ex></expan></w> <w xml:id="caes0002-6" xml:lang="la">Callisto</w> <w xml:id="caes0002-7" xml:lang="la"><expan><abbr>c</abbr><ex>ausa</ex></expan>,</w> <w xml:id="caes0002-8" xml:lang="la"><expan><abbr>C</abbr><ex>aius</ex></expan></w> <w xml:id="caes0002-9" xml:lang="la">Aurunculeius</w> <w xml:id="caes0002-10" xml:lang="la">amico</w> <w xml:id="caes0002-11" xml:lang="la">suo.</w></p>
</div>
