                  <decoDesc>
                     <decoNote ana="#xx">
                        <ab/>
                        <locus/>
                     </decoNote>
                     <decoNote ana="#xx">
                        <ab>A cross</ab>
                        <locus>cut into the last sigma at the end of line 3</locus>
                     </decoNote>
                  </decoDesc>
               </physDesc>
