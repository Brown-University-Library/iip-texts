               <msContents>
                  <textLang mainLang="grc"/>
                  <msItem ana="#christian" class="#acclamation">
                     <p>Caesarea, Late sixth or early seventh century CE. Paved Mosaic.
                        Acclamation.</p>
                  </msItem>
               </msContents>
               <!-- ********* <physDesc> ********* -->
               <physDesc><!-- ********* <objectDesc> ********* -->
                  <objectDesc ana="pavement #mosaic">
                     <supportDesc ana="stone.other">
                        <support>
                           <dimensions type="surface" unit="cm">
                              <height/>
                              <width>104</width>
                           </dimensions>
                           <p>Dimensions refer to the diameter</p>
                        </support>
                        <condition ana="complete.broken">
                           <p>Line 1 is badly preserved, but all letters except the first were
                              legible when the inscription was unearthed and the reading is
                              certain</p>
                        </condition>
                     </supportDesc>
                     <!-- ********* <layoutDesc> ********* -->
                     <layoutDesc>
                        <layout columns="1" writtenLines="4">
                           <p/>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <!-- ********* <handDesc> ********* -->
                  <handDesc>
                     <handNote ana="#engraved">
                        <dimensions type="letter" extent="height" unit="cm" atLeast="8.7" atMost="9.5"/>
                     </handNote>
                  </handDesc>
                  <!-- ********* <decoDesc> ********* -->
                  <decoDesc>
                     <decoNote ana="#xx">
                        <ab>Medallion</ab>
                        <locus>Around the inscription</locus>
                     </decoNote>
                     <decoNote>
                        <ab>Square frame made of five rows</ab>
                        <locus>Interlacing the medallion at cardinal points</locus>
                     </decoNote>
                     <decoNote>
                        <ab>Two leaves</ab>
                        <locus>Below the text, within the medallion</locus>
                     </decoNote>
                     <decoNote>
                        <ab>Multicolored tesserae in diagonal rows</ab>
                        <locus>At the corners of the square frame</locus>
                     </decoNote>
                  </decoDesc>
               </physDesc>
               <!-- ********* <history> ********* -->
               <history>
                  <origin>
                     <date period="http://n2t.net/ark:/99152/p0m63njjcn4" notBefore="0575" notAfter="0700">Late sixth or early seventh century
                        CE</date>
                     <placeName>
                        <region>Coastal Plain</region>
                        <settlement ref="http://pleiades.stoa.org/places/678401">Caesarea</settlement>
                        <geogName type="site"/>
                        <geogFeat type="locus">Area I1</geogFeat>
                     </placeName>
                     <p>Found in the Inner Harbor in 1992, in the pavement of a room of uncertain size and function just north of the monumental
                        staircase leading upward to the Temple Platform.</p>
                  </origin>
                  <provenance>
