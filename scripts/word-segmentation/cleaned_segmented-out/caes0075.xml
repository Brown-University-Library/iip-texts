               <!-- ********* <physDesc> ********* -->
               <physDesc><!-- ********* <objectDesc> ********* -->
                  <objectDesc ana="#block">
                     <supportDesc ana="marble">
                        <support>
                           <dimensions type="surface" unit="cm">
                              <height>26</height>
                              <width>48</width>
                              <depth>6</depth>
                           </dimensions>
                        </support>
                        <condition ana="fragment.single">
                           <p>Broken at both ends. It is flat and smooth on the top and flat and
                              pointed on the bottom. Both sides are molded, the back in simple
                              planes, finished with a point and, toward the bottom, smoothed; the
                              front in a doubled cyma finished with a claw chisel and, again toward
                              the bottom, smoothed.</p>
                        </condition>
                     </supportDesc>
                     <!-- ********* <layoutDesc> ********* -->
                     <layoutDesc>
                        <layout columns="1" writtenLines="1">
                           <p>The vertical curve of the cyma, above the portion that is smoothed,
                              carries part of the single line of an inscription</p>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
