               <!-- ********* <msContents> ********* -->
               <msContents>
                  <textLang mainLang="grc"/>
                  <msItem ana="christian" class="dedicatory">
                     <p>Galilee. Hammatha. 300 CE - 700 CE. Slab. Dedicatory. </p>
                  </msItem>
               </msContents>
               <!-- ********* <physDesc> ********* -->
               <physDesc><!-- ********* <objectDesc> ********* -->
                  <objectDesc ana="#slab">
                     <supportDesc ana="marble">
                        <support>
                           <dimensions type="surface" unit="cm">
                              <height>99.6</height>
                              <width>104</width>
                              <depth>2.3</depth>
                           </dimensions>
                        </support>
                        <condition ana="complete.broken">
                           <p>The left side of the plaque is missing, as well as the upper right
                              corner. Part of it may have been hewn off in antiquity, since the
                              first line seems to have been arranged to fit into the available space
                              and ends with an ivy leaf serving as an abbreviation</p>
                        </condition>
                     </supportDesc>
                     <!-- ********* <layoutDesc> ********* -->
                     <layoutDesc>
                        <layout columns="1" writtenLines="4">
                           <p>The script is oriented to the south, toward the center of the hall</p>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <!-- ********* <handDesc> ********* -->
                  <handDesc>
                     <handNote ana="#engraved">
                        <dimensions type="letter" extent="height" unit="cm" atLeast="7" atMost="10"/><p>The letters are exceptionally large, sharply chiselled and elegantly shaped, although they are marred by cuts that disfigure several characters</p>;
                     </handNote>
                  </handDesc>
                  <!-- ********* <decoDesc> ********* -->
                  <decoDesc>
                     <decoNote ana="#xx">
                        <ab> Ivy leaf. </ab>
                        <locus>At the end of the first line of the inscription. </locus>
                     </decoNote>
                  </decoDesc>
               </physDesc>
