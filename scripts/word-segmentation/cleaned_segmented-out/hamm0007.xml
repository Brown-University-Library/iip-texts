                  <objectDesc ana="#slab">
                     <supportDesc ana="#marble">
                        <support>
                           <p>Whitish-grey marble, engraved in bathhouse floor. It adjoins hamm0006
                              to the east</p>
                           <dimensions type="surface" unit="cm">
                              <height>69.5</height>
                              <width>72</width>
                              <depth>3.2</depth>
                           </dimensions>
                        </support>
                        <condition ana="fragments.contig">
                           <p>The stone surface is much worn by the rubbing of feet and the first
                              two lines are poorly preserved</p>
                        </condition>
                     </supportDesc>
                     <!-- ********* <layoutDesc> ********* -->
