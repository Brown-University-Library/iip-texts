               <physDesc><!-- ********* <objectDesc> ********* -->
                  <objectDesc ana="#plaque">
                     <supportDesc ana="#marble">
                        <support><p>Upper left corner of a plaque of white marble</p>
                           <dimensions type="surface" unit="cm">
                              <height>12</height>
                              <width>12</width>
                              <depth>ca. 3</depth>
                           </dimensions>
                        </support>
                        <condition ana="fragment.single">
                           <p>The face and forward portions of the sides are smooth, the back
                              rough</p>
                        </condition>
                     </supportDesc>
                     <!-- ********* <layoutDesc> ********* -->
                     <layoutDesc>
                        <layout columns="1" writtenLines="3">
                           <p>The beginning of the first three lines of an epitaph survive, with
                              vestiges of a fourth below. The first line is centered</p>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <!-- ********* <handDesc> ********* -->
                  <handDesc>
                     <handNote ana="#engraved">
                        <dimensions type="letter" extent="height" unit="cm" atLeast="2.2" atMost="2.8"/><p>Letters, with diminutive serifs, are irregular but deeply cut within (but not strictly aligned
                           to) guidelines 31-32mm apart, one to each line. Alphas have broken
                           bars</p>
                     </handNote>
                  </handDesc>
                  <!-- ********* <decoDesc> ********* -->
                  <decoDesc>
                     <decoNote ana="#xx">
                        <ab> Menorah </ab>
                        <locus> Upper left margin of the plaque </locus>
                     </decoNote>
                     <decoNote ana="#xx">
                        <ab> Shofar </ab>
                        <locus> Upper left margin of the plaque, on the bottom right of the menorah </locus>
                     </decoNote>
                     <decoNote ana="#xx">
                        <ab> Lulav </ab>
                        <locus> Upper left margin of the plaque, on the bottom left of the menorah </locus>
                     </decoNote>
                  </decoDesc>
