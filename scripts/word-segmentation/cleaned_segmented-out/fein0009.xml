               <msContents>
                  <textLang mainLang="grc"/>
                  <msItem class="#funerary.epitaph" ana="#christian">
                     <p>Feinan, Late 6th century CE. Stele. Funerary (Epitaph).</p>
                  </msItem>
               </msContents>
               <!-- ********* <physDesc> ********* -->
               <physDesc>
                  <!-- ********* <objectDesc> ********* -->
                  <objectDesc ana="#stele">
                     <supportDesc ana="#stone.other">
                        <support>
                           <p>Reddish-white sandstone, almost square in shape with rounded
                              corners</p>
                           <dimensions type="surface" unit="cm">
                              <height>39</height>
                              <width>26</width>
                              <depth/>
                           </dimensions>
                        </support>
                        <condition ana="#complete.broken">
                           <p>Broken in the lower right corner</p>
                        </condition>
                     </supportDesc>
                     <!-- ********* <layoutDesc> ********* -->
                     <layoutDesc>
                        <layout columns="1" writtenLines="3">
                           <p>The first line is separated in two sections by the lower part of the
                              cross, while the second line lies just below the decorative motif</p>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <!-- ********* <handDesc> ********* -->
                  <handDesc>
                     <handNote ana="#impressed.inscribed">
                        <dimensions type="letter" extent="height" atLeast="1.5" atMost="5"/>
                        <p>Oval alphabet, with some letters especially elongated</p>
                     </handNote>
                  </handDesc>
                  <!-- ********* <decoDesc> ********* -->
                  <decoDesc>
                     <decoNote ana="#xx">
                        <ab>Big deeply engraved cross which ends at a triangular pedestal</ab>
                        <locus>Above and within line one</locus>
                     </decoNote>
                     <decoNote>
                        <ab>Semicircle bearing a deeply incised dot in each of its two
                           quadrants</ab>
                        <locus>Enclosing the upper part of the cross</locus>
                     </decoNote>
                  </decoDesc>
               </physDesc>
