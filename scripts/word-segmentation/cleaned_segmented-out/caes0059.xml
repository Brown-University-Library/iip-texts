                  <msItem ana="other_religion" class="#dedicatory">
                     <p>Caesarea, 546 CE - 606 CE. Pavement (Mosaic). Dedicatory.</p>
                  </msItem>
               </msContents>
               <!-- ********* <physDesc> ********* -->
               <physDesc><!-- ********* <objectDesc> ********* -->
                  <objectDesc ana="pavement #mosaic">
                     <supportDesc ana="stone.other">
                        <support>
                           <dimensions type="surface" unit="cm">
                              <height>124</height>
                              <width>435</width>
                           </dimensions>
                        </support>
                        <condition ana="complete">
                           <p/>
                        </condition>
                     </supportDesc>
                     <!-- ********* <layoutDesc> ********* -->
                     <layoutDesc>
                        <layout columns="1" writtenLines="6">
                           <p>The borders and letters consist of single rows of black tesserae in a
                              field of off-white. The tesserae are large and irregular</p>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <!-- ********* <handDesc> ********* -->
                  <handDesc>
                     <handNote ana="#engraved">
                        <dimensions type="letter" extent="height" unit="cm" atLeast="12" atMost="20"/>
                     </handNote>
                  </handDesc>
                  <!-- ********* <decoDesc> ********* -->
                  <decoDesc>
                     <decoNote ana="#xx">
                        <ab>Rosettes and double borders</ab>
                        <locus>Ansae</locus>
                     </decoNote>
                     <decoNote ana="#xx">
                        <ab>Curved lines </ab>
                        <locus>Beginning and end of line 6</locus>
                     </decoNote>
                     <decoNote>
                        <ab>Tabula ansata oriented with the top to the north and lies just below a
                           flight of ten steps, whose width it extends</ab>
                        <locus>Around the inscription</locus>
                     </decoNote>
                  </decoDesc>
               </physDesc>
